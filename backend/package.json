{
  "name": "backend",
  "version": "1.0.0",
  "description": "LogistiMa Backend API",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "dev:worker": "tsx watch src/workers/worker.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:worker": "node dist/workers/worker.js",
    "test": "cross-env NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" jest --passWithNoTests",
    "test:coverage": "cross-env NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" jest --coverage",
    "migrate": "tsx src/migrations/index.ts",
    "migrate:down": "tsx src/migrations/index.ts down",
    "seed": "tsx src/seeders/index.ts",
    "db:reset": "pnpm migrate:down && pnpm migrate && pnpm seed"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.27.0",
  "dependencies": {
    "bullmq": "^5.66.4",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-async-errors": "^3.1.1",
    "ioredis": "^5.8.2",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "@types/sequelize": "^6.12.0",
    "@types/uuid": "^11.0.0",
    "cross-env": "^10.1.0",
    "jest": "^30.2.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}